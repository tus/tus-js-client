{
  "include": ["lib/**/*"],
  "compilerOptions": {
    "target": "ES2018",
    "module": "Node16",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": false,
    "sourceMap": true,
    "noImplicitAny": true
  }
}
